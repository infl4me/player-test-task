{
  "plugins": [
    "babel",
    "cypress"
  ],
  "env": {
    "node": true,
    "browser": true,
    "cypress/globals": true
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "no-console": 0,
    "no-param-reassign": ["error", { "props": false }]
  }
}